# Development Environment Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/appointment_test_db?schema=public

# AWS LocalStack Configuration for Development
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_ENDPOINT_URL=http://localhost:4566

# SQS Configuration for LocalStack
SQS_APPOINTMENT_QUEUE_URL=http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/appointment-queue
SQS_APPOINTMENT_DLQ_URL=http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/appointment-queue-dlq

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Security & Rate Limiting
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# SQS Tuning for Development
SQS_VISIBILITY_TIMEOUT=300
SQS_BATCH_SIZE=10
SQS_WAIT_TIME=20
SQS_MAX_RECEIVE_COUNT=3

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=
CORRELATION_ID_HEADER=x-correlation-id
